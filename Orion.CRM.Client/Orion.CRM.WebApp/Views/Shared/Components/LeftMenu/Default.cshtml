@model IEnumerable<Orion.CRM.WebApp.Models.Role.RoleMenuComplex>

@if (Model != null) {
    <ul class="nav nav-list bs-docs-sidenav nav-collapse collapse">
        @foreach(var menu in Model) {
            if (menu.MenuId == ViewBag.LeftMenuId) {
                <li class="active">
                    <a href="@menu.URL"><i class="@menu.Icon"></i>@menu.MenuName</a>
                </li>
            }
            else {
                <li>
                    <a href="@menu.URL"><i class="@menu.Icon"></i>@menu.MenuName</a>
                </li>
            }       
        }
    </ul>
}

