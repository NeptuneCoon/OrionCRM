
@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section styles{
    <style>
        input[type="radio"], input[type="checkbox"] {
            margin: 0 0 0;
            line-height: normal;
        }

        .search-input {
            border: 1px solid gray;
            border-radius: 1px !important;
            padding: 2px 3px;
            height: 16px !important;
            width: 91px;
            font-size: 12px !important;
        }

        .search-select {
            width: 105px;
            height: 27px;
            border-radius: 1px !important;
            padding: 0px 3px
        }
    </style>
}

<div class="panel panel-default" style="width:1000px">
    <div class="panel-heading">
        <h3 class="panel-title">资源列表</h3>
    </div>

    <div class="panel-body">
        <div class="form-inline">
            <label>姓名</label>
            <input type="text" class="search-input" />
            <label style="margin-left:5px">手机/QQ/微信</label>
            <input type="text" class="search-input" />
            <select name="Project" class="search-select">
                <option>多淘天下</option>
                <option>华域商城</option>
            </select>
            <select name="Project" class="search-select">
                <option>业务组</option>
                <option>业务一组</option>
            </select>
            <select name="Project" class="search-select">
                <option>选择业务员</option>
                <option>张涛</option>
            </select>
            <select name="Project" class="search-select">
                <option>状态</option>
                <option>洽谈中</option>
            </select>
            <select name="TalkCount" class="search-select">
                <option value="">洽谈次数</option>
                <option value="0">0次</option>
                <option value="1">1次</option>
                <option value="2">2次</option>
                <option value="3">3次</option>
                <option value="4">4次</option>
                <option value="5">&gt;5次</option>
                <option value="10">&gt;10次</option>
            </select>
            <select name="Project" class="search-select">
                <option>意向</option>
                <option>意向强烈</option>
            </select>

        </div>
        <div class="form-inline" style="margin-top:5px">
            <label>来源</label>
            <select name="Project" class="search-select">
                <option>来源</option>
                <option>客服分配资源</option>
            </select>
            <select name="Project" class="search-select">
                <option>分配状态</option>
                <option value="1">管理未分</option>
                <option value="2">组长未分</option>
            </select>
            <button type="button" class="btn btn-default">
                <i class="icon-search"></i> 查询
            </button>
            <button type="button" class="btn btn-default">批量分配</button>
        </div>

